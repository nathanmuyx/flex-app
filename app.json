{
  "expo": {
    "name": "flex",
    "slug": "flex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "flex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/collegehunklogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#e87200"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/collegehunklogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#e87200"
      },
      "bundleIdentifier": "com.anonymous.flex"
    },
    "android": {
      "splash": {
        "image": "./assets/images/collegehunklogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#e87200"
      },
      "adaptiveIcon": {
        "backgroundColor": "#e87200",
        "foregroundImage": "./assets/images/collegehunklogo.png",
        "monochromeImage": "./assets/images/collegehunklogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.flex"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/collegehunklogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#e87200"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Flex to access your photos to create posts and stories.",
          "savePhotosPermission": "Allow Flex to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow Flex to access your camera to take photos and videos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
